{
    "name": "werkzeug",
    "version": "0.0.11",
    "description": "compiles ts, coffee, sass/scss, less/ stylus and packs browser ready bundles",
    "main": "./js/werkzeug.js",
    "bin": {
        "wz": "./bin/werkzeug"
    },
    "scripts": {
        "postinstall": "coffee -c -o ./js ./coffee",
        "start": "coffee -c -w -o ./js ./coffee"
    },
    "keywords": [
        "werkzeug",
        "compiler",
        "packer",
        "bundler",
        "bundles",
        "typescript",
        "coffeescript",
        "sass",
        "less",
        "stylus"
    ],
    "author": "jarends",
    "homepage": "https://github.com/jarends/werkzeug#README",
    "repository": {
        "type": "git",
        "url": "https://github.com/jarends/werkzeug.git"
    },
    "bugs": {
        "url": "https://github.com/jarends/werkzeug/issues"
    },
    "license": "Unlicense",
    "devDependencies": {},
    "dependencies": {
        "babel-core": "6.18.2",
        "babel-preset-es2015": "6.18.0",
        "body-parser": "1.15.2",
        "chokidar": "1.6.0",
        "coffee": "3.3.0",
        "colors": "1.1.2",
        "cross-spawn": "4.0.0",
        "emap": "0.0.5",
        "express": "4.14.0",
        "fs-extra": "0.30.0",
        "homedir": "0.6.0",
        "jsdictionary": "0.0.2",
        "jsonminify": "0.4.1",
        "karg": "1.0.9",
        "less": "2.7.1",
        "lodash": "4.13.1",
        "ng-annotate": "1.2.1",
        "node-sass": "3.11.2",
        "portfinder": "1.0.9",
        "strip-ansi": "3.0.1",
        "stylus": "0.54.5",
        "tslib": "1.0.0",
        "tslint": "3.15.1",
        "typescript": "2.1.1",
        "uglify-js": "^2.5.0",
        "walkdir": "0.0.11"
    }
}
